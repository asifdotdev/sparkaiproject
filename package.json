{
  "name": "sparkaiproject",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/shared",
    "packages/backend",
    "packages/mobile-app",
    "packages/admin-panel"
  ],
  "scripts": {
    "shared:build": "npm run build --workspace=@sparkai/shared",
    "backend:dev": "npm run dev --workspace=@sparkai/backend",
    "backend:build": "npm run build --workspace=@sparkai/backend",
    "backend:seed": "npm run seed --workspace=@sparkai/backend",
    "mobile:start": "npm run start --workspace=@sparkai/mobile-app",
    "admin:dev": "npm run dev --workspace=@sparkai/admin-panel",
    "admin:build": "npm run build --workspace=@sparkai/admin-panel"
  }
}
